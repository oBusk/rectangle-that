{
  "name": "rectangle-that",
  "private": true,
  "version": "0.0.1",
  "description": "A blatant ripoff",
  "author": "Oscar Busk <oscar.busk@gmail.com>",
  "engines": {
    "node": "24.x"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "^24.11.1",
      "onFail": "download"
    }
  },
  "packageManager": "pnpm@10.24.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc && pnpm run style && cp index.template.html dist/index.html && cp manifest.json dist/manifest.json ",
    "serve": "serve dist",
    "start": "pnpm run build && concurrently \"pnpm run tsc -w\" \"pnpm run serve\" \"pnpm run style:w\"",
    "style": "lessc style.less dist/style.css",
    "style:w": "chokidar style.less -c \"pnpm run style\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc",
    "lint": "tslint -p tsconfig.json",
    "lint-fix": "tslint -p tsconfig.json --fix"
  },
  "dependencies": {
    "less": "^4.1.2",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.14",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.1.0",
    "serve": "^13.0.2",
    "tslint": "^6.1.3"
  }
}
